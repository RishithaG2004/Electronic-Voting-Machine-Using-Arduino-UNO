## Arduino Code 
<pre>
```cpp
#include <iostream>
using namespace std;

int main() {
    cout << "Hello, GitHub!" << endl;
    return 0;
}
```
</pre>
```cpp
#include <LiquidCrystal.h>
#include <LiquidCrystal.h>

// Initialize the LCD (RS, E, D4, D5, D6, D7)
LiquidCrystal lcd(13, 12, 11, 10, 9, 8);

// Define button pins
#define S1 7  // Candidate A
#define S2 6  // Candidate B
#define S3 5  // Candidate C
#define S4 4  // Candidate D
#define S5 3  // Result Button

// Vote counters
int vote1 = 0;
int vote2 = 0;
int vote3 = 0;
int vote4 = 0;

void setup() {
  // Set button pins as input
  pinMode(S1, INPUT);
  pinMode(S2, INPUT);
  pinMode(S3, INPUT);
  pinMode(S4, INPUT);
  pinMode(S5, INPUT);

  // Initialize LCD
  lcd.begin(16, 2);
  lcd.print(" Electronic ");
  lcd.setCursor(0, 1);
  lcd.print(" Voting Machine ");
  delay(4000);

  // Enable internal pull-up resistors
  digitalWrite(S1, HIGH);
  digitalWrite(S2, HIGH);
  digitalWrite(S3, HIGH);
  digitalWrite(S4, HIGH);
  digitalWrite(S5, HIGH);

  lcd.clear();
  lcd.setCursor(1, 0);
  lcd.print("A");
  lcd.setCursor(5, 0);
  lcd.print("B");
  lcd.setCursor(9, 0);
  lcd.print("C");
  lcd.setCursor(13, 0);
  lcd.print("D");
}

void loop() {
  // Display vote counts
  lcd.setCursor(1, 1);
  lcd.print(vote1);
  lcd.setCursor(5, 1);
  lcd.print(vote2);
  lcd.setCursor(9, 1);
  lcd.print(vote3);
  lcd.setCursor(13, 1);
  lcd.print(vote4);

  // Check for button presses and increment votes
  if (digitalRead(S1) == LOW) {
    vote1++;
    while (digitalRead(S1) == LOW);
  }
  if (digitalRead(S2) == LOW) {
    vote2++;
    while (digitalRead(S2) == LOW);
  }
  if (digitalRead(S3) == LOW) {
    vote3++;
    while (digitalRead(S3) == LOW);
  }
  if (digitalRead(S4) == LOW) {
    vote4++;
    while (digitalRead(S4) == LOW);
  }

  // Display result when result button is pressed
  if (digitalRead(S5) == LOW) {
    int totalVotes = vote1 + vote2 + vote3 + vote4;
    if (totalVotes > 0) {
      lcd.clear();
      if (vote1 > vote2 && vote1 > vote3 && vote1 > vote4) {
        lcd.print("A is Winner");
      } else if (vote2 > vote1 && vote2 > vote3 && vote2 > vote4) {
        lcd.print("B is Winner");
      } else if (vote3 > vote1 && vote3 > vote2 && vote3 > vote4) {
        lcd.print("C is Winner");
      } else if (vote4 > vote1 && vote4 > vote2 && vote4 > vote3) {
        lcd.print("D is Winner");
      } else {
        lcd.print("Tie or No Result");
      }
    } else {
      lcd.clear();
      lcd.print("No Voting....");
    }
    delay(3000);
    // Reset vote counts
    vote1 = vote2 = vote3 = vote4 = 0;
    lcd.clear();
  }
}
